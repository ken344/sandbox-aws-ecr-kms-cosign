# Terraform Variables Example
# 
# このファイルを terraform.tfvars にコピーして使用してください
# cp terraform.tfvars.example terraform.tfvars

# AWS設定
aws_region = "ap-northeast-1"

# tfstate用S3バケット名
# setup-tfstate-bucket.sh で作成したバケット名を指定
tfstate_bucket_name = "sandbox-ecr-kms-tfstate-123456789012-ap-northeast-1"

# GitHub情報
# あなたのGitHub組織名またはユーザー名
github_org = "your-github-username"

# リポジトリ名
github_repo = "sandbox-aws-ecr-kms-cosign"

# プロジェクト設定
project_name = "sandbox-ecr-kms"
environment  = "dev"

